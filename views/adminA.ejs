<%- include('layout/header') -%>
    <div class="wrapper">
        <div class="container-fluid">

            <div class="row">

                <div class="col-md-12 col-xl-8">
                    <div class="card m-b-30 h-360">
                        <div class="card-body">
                            <h5 class="header-title mt-0 pb-3">Monitor Antrian</h5>
                            <h5 class="header-title mt-0 pb-3">
                                <%= user %>
                            </h5>
                            <div class="row">
                                <div class="col-md-12 col-xl-4">
                                    <div class="card m-b-30 text-center">
                                        <div class="h2 card-header text-white bg-success">
                                            Loket 1 A
                                        </div>
                                        <div class="card-body">
                                            <h1 id="A 1">0</h1>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-12 col-xl-4">
                                    <div class="card m-b-30 text-center">
                                        <div class="h2 card-header text-white bg-success">
                                            Loket 2 A
                                        </div>
                                        <div class="card-body">
                                            <h1 id="A 2">0</h1>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-12 col-xl-4">
                                    <div class="card m-b-30 text-center">
                                        <div class="h2 card-header text-white bg-success">
                                            Loket 3 A
                                        </div>
                                        <div class="card-body">
                                            <h1 id="A 3">0</h1>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-12 col-xl-4">
                                    <div class="card m-b-30 text-center">
                                        <div class="h2 card-header text-white bg-success">
                                            Loket 4 A
                                        </div>
                                        <div class="card-body">
                                            <h1 id="A 4">0</h1>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-md-12 col-xl-4">
                    <div class="row">
                        <div class="card m-b-30 border-0 box-pangil">
                            <div class="card-body  text-center">
                                <div class="row text-center profile-block">
                                    <div class="col-4 align-self-center py-2 border-right">
                                            <div class="card-body">
                                                <button class="btn btn-block btn-raised btn-danger" id="reset"
                                                    onclick="resetLoket()"></i> Reset Loket</button>
                                            </div>
                                    </div>
                                    <div class="col-4 align-self-center py-2 border-right">
                                        <h3 class="profile-count">
                                            <b class="font-22" id="sisa">0</b>
                                        </h3>
                                        <p class="mb-0">Sisa Antrian</p>
                                    </div>
                                    <div class="col-4 align-self-center py-2">
                                        <h3 class="profile-count">
                                            <b class="font-22" id="total">0</b>
                                        </h3>
                                        <p class="mb-0">Total Antrian</p>
                                    </div>
                                </div>
                                <div class="text-center pt-1">
                                    <button class="btn btn-block btn-raised btn-primary mb-3" id="next"
                                        onclick="next('<%-privilege%>')"><i class="mdi mdi-voice"></i> Panggil
                                        Berikutnya</button>
                                    <button class="btn btn-block btn-raised btn-info mb-3" id="ulang"
                                        onclick="ulangPangilan('<%-privilege%>')"><i class="mdi mdi-voice"></i>
                                        Panggil
                                        Ulang</button>
                                </div>

                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div> <!-- end container -->
    </div>
    <!-- end wrapper -->
    <%- include('layout/script') -%>
    <script>
        let level = '<%-privilege%>';
      </script>
    <script src="/asset/js/audio.js"></script>
    <script src="/asset/js/adminA.js"></script>
        <%- include('layout/footer') -%>