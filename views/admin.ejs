<%- include('layout/header') -%>
    <div class="wrapper">
        <div class="container-fluid">

            <div class="row">

                <div class="col-md-12 col-xl-8">
                    <div class="card m-b-30 h-360">
                        <div class="card-body">
                            <h5 class="header-title mt-0 pb-3">Monitor Antrian</h5>
                            <h5 class="header-title mt-0 pb-3">
                                <%= loket %>
                            </h5>
                            <div class="row">
                                <div class="col-md-12 col-xl-4">
                                    <div class="card m-b-30 text-center">
                                        <div class="h2 card-header text-white bg-success">
                                            Loket 1
                                        </div>
                                        <div class="card-body">
                                            <h1 id="1">0</h1>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-12 col-xl-4">
                                    <div class="card m-b-30 text-center">
                                        <div class="h2 card-header text-white bg-success">
                                            Loket 2
                                        </div>
                                        <div class="card-body">
                                            <h1 id="2">0</h1>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-12 col-xl-4">
                                    <div class="card m-b-30 text-center">
                                        <div class="h2 card-header text-white bg-success">
                                            Loket 3
                                        </div>
                                        <div class="card-body">
                                            <h1 id="3">0</h1>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-12 col-xl-4">
                                    <div class="card m-b-30 text-center">
                                        <div class="h2 card-header text-white bg-success">
                                            Loket 4
                                        </div>
                                        <div class="card-body">
                                            <h1 id="4">0</h1>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-md-12 col-xl-4">
                    <div class="row">
                        <div class="card m-b-30 border-0 box-pangil">
                            <div class="card-body  text-center">
                                <div class="row text-center profile-block">
                                    <div class="col-4 align-self-center py-2 border-right">
                                        <!-- <img src="/asset/img/logo-RSUD.jpg" alt=""
                                            class="rounded-circle mx-auto d-block w-25"> -->
                                            <div class="card-body">
                                                <button class="btn btn-block btn-raised btn-danger" id="reset"
                                                    onclick="resetLoket()"></i> Reset Loket</button>
                                            </div>
                                    </div>
                                    <div class="col-4 align-self-center py-2 border-right">
                                        <h3 class="profile-count">
                                            <b class="font-22" id="sisa">0</b>
                                        </h3>
                                        <p class="mb-0">Sisa Antrian</p>
                                    </div>
                                    <div class="col-4 align-self-center py-2">
                                        <h3 class="profile-count">
                                            <b class="font-22" id="total">0</b>
                                        </h3>
                                        <p class="mb-0">Total Antrian</p>
                                    </div>
                                </div>
                                <div class="text-center pt-1">
                                    <button class="btn btn-block btn-raised btn-primary mb-3" id="next"
                                        onclick="next('<%-privilege%>')"><i class="mdi mdi-voice"></i> Panggil
                                        Berikutnya</button>
                                    <button class="btn btn-block btn-raised btn-info mb-3" id="ulang"
                                        onclick="ulangPangilan('<%-privilege%>')"><i class="mdi mdi-voice"></i>
                                        Panggil
                                        Ulang</button>
                                    <!-- <button class="btn btn-block btn-raised btn-success mb-3"><i
                                    class="mdi mdi-account-check"></i> Selesai Dilayani</button>
                            <button class="btn btn-block btn-raised btn-danger mb-3"><i
                                    class="mdi mdi-account-remove"></i> Tidak Hadir</button>
                            <button class="btn btn-block btn-raised text-white bg-dark mb-3"><i
                                    class="mdi mdi-account-convert"></i> Lewati</button> -->
                                </div>

                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="card m-b-30 border-0 box-pangil">
                            <div class="card-body  text-center">
                                <div class="row text-center profile-block">
                                    <div class="col-4 align-self-center py-2 border-right">
                                        <img src="/asset/img/logo-RSUD.jpg" alt=""
                                            class="rounded-circle mx-auto d-block w-25">
                                    </div>
                                    <div class="col-4 align-self-center py-2 border-right">
                                        <h3 class="profile-count">
                                            <b class="font-22" id="sisaPrioritas">0</b>
                                        </h3>
                                        <p class="mb-0">Sisa Antrian Prioritas</p>
                                    </div>
                                    <div class="col-4 align-self-center py-2">
                                        <h3 class="profile-count">
                                            <b class="font-22" id="totalPrioritas">0</b>
                                        </h3>
                                        <p class="mb-0">Total Antrian Prioritas</p>
                                    </div>
                                </div>
                                <div class="text-center pt-1">
                                    <button class="btn btn-block btn-raised btn-primary mb-3" id="nextPrioritas"
                                        onclick="nextPrioritas('<%-privilege%>')"><i class="mdi mdi-voice"></i> Panggil
                                        Berikutnya</button>
                                    <button class="btn btn-block btn-raised btn-info mb-3" id="ulangPrioritas"
                                        onclick="ulangPangilanPrioritas('<%-privilege%>')"><i class="mdi mdi-voice"></i>
                                        Panggil
                                        Ulang
                                    </button>
                
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div> <!-- end container -->
    </div>
    <!-- end wrapper -->
    <%- include('layout/script') -%>
    <script>
        let level = '<%-privilege%>';
      </script>
    <script src="/asset/js/audio.js"></script>
    <script src="/asset/js/admin.js"></script>
        <%- include('layout/footer') -%>